<Window x:Class="Parser2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Parser2"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="25" />
        </Grid.RowDefinitions>
        <WebBrowser Name="web" />
        <TextBox Grid.Row="1" Name="Txt" TextWrapping="Wrap" IsReadOnly="True" VerticalScrollBarVisibility="Visible" />
        <Button Grid.Row="2" Content="START" Click="Button_Click" />
    </Grid>
</Window>
